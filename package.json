{
  "name": "react-webpack-starter",
  "version": "1.0.0",
  "description": "starter template project for react, react-router, browserHistory, IndexRedirect, es6 babel webpack",
  "main": "webpack.config.js",
  "scripts": {
    "prewebpack": "npm i --no-optional",
    "webpack": "webpack",
    "test": "webpack -w",
    "dev": "webpack-dev-server --content base ./",
    "start": "npm run webpack"
  },
  "author": "Maksim Kostromin",
  "license": "ISC",
  "dependencies": {
    "bootstrap": "^3.3.6",
    "react": "^0.14.7",
    "react-dom": "^0.14.7",
    "react-router": "^2.0.1"
  },
  "devDependencies": {
    "babel": "^6.5.2",
    "babel-core": "^6.7.2",
    "babel-loader": "^6.2.4",
    "babel-plugin-add-module-exports": "^0.1.2",
    "babel-plugin-react-html-attrs": "^2.0.0",
    "babel-plugin-transform-class-properties": "^6.6.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "css-loader": "^0.23.1",
    "file-loader": "^0.8.5",
    "style-loader": "^0.13.0",
    "url-loader": "^0.5.7",
    "webpack": "^1.12.14",
    "webpack-dev-server": "^1.14.1"
  },
  "tags": [
    "react",
    "react-router",
    "history",
    "hashHistory",
    "es6",
    "babel",
    "webpack"
  ],
  "babel": {
    "presets": [
      "react",
      "es2015"
    ],
    "plugins": [
      "react-html-attrs",
      "transform-class-properties",
      "transform-decorators-legacy"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/daggerok/react-webpack-starter.git"
  },
  "keywords": [
    "react",
    "react-router",
    "browser",
    "hystory",
    "es6",
    "babel",
    "webpack"
  ],
  "bugs": {
    "url": "https://github.com/daggerok/react-webpack-starter/issues"
  },
  "homepage": "https://github.com/daggerok/react-webpack-starter#readme"
}
