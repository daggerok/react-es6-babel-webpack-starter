# EditorConfig is awesome: http://EditorConfig.org

# top-most EditorConfig file
root = true

[*]
ident_size = 2
ident_style = space
insert_final_new_line = true
charset = utf-8
trim_trailing_whitespace = true
tab_width = 2
end_of_line = lf

[*.md]
indent_style = tab
trim_trailing_whitespace = false
